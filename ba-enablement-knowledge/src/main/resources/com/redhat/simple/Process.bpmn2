      <bpmn2:outgoing>SequenceFlow_8</bpmn2:outgoing>
      <bpmn2:script>System.out.println(&quot;It's over 9000!!!!!&quot;);</bpmn2:script>
    </bpmn2:scriptTask>
    <bpmn2:sequenceFlow id="SequenceFlow_7" tns:priority="1" name="over 9000" sourceRef="ExclusiveGateway_2" targetRef="ScriptTask_3">
